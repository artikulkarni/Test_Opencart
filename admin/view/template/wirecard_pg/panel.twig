{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <h1>{{ heading_title }}</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
    <div class="container-fluid">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-puzzle-piece"></i> {{ text_list }}</h3>
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <td class="text-left">Transaction</td>
                                <td class="text-left">Order number</td>
                                <td class="text-left">Transaction ID</td>
                                <td class="text-left">Parent transaction ID</td>
                                <td class="text-left">Action</td>
                                <td class="text-left">Payment method</td>
                                <td class="text-left">Transaction state</td>
                                <td class="text-left">Amount</td>
                                <td class="text-left">Currency</td>
                                <td class="text-right">Details</td>
                            </tr>
                        </thead>
                        <tbody>

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
{{ footer }}